<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.cityu.dao.CodeMapper">
  <select id="getUserRoles" resultType="java.util.Map">
    select code, title
    from c_role
  </select>
  <select id="getTaxRefundMethods" resultType="java.util.Map">
    select code, title
    from c_tax_refund_method
  </select>
  <select id="getStatus" parameterType="java.lang.String" resultType="java.util.Map">
    select code, title
    from c_status
    where business = #{business,jdbcType=VARCHAR}
  </select>
</mapper>